CREATE TABLE Automovel (
    Placa             VARCHAR(10) PRIMARY KEY,
    Modelo            VARCHAR(100),
    Ano               INTEGER,
    NomeMontadora     VARCHAR(100),
    SiteMontadora     VARCHAR(255),
    LogotipoMontadora VARCHAR(255)
);

